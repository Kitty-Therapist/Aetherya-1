<%- include('../blocks/header', {bot, user, path}) %>
<h2>User Information</h2>
<div class="row">
  <div class="col col-4">
    <%- include('../blocks/user-card') %>
    <ul class="list-group list-group-flush">
      <li><%= user.createdTimestamp%></li>
    </ul>
  </div>
</div>
<script type="text/javascript">
  var ts = parseInt($("#userCreate").text());
  $("#userCreate").html(`<strong>Created</strong> : ${moment(ts).format('MMMM Do YYYY (h:mm a)')}`)
</script>
<% include ../blocks/footer %>